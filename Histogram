#include <stdio.h>
#include <string.h>
#include <ctype.h>

int alphaKey[26];


int main() {
memset(alphaKey,0,26*sizeof(alphaKey[0]));
FILE* input = stdin;
FILE* output = stdout;
char c;
while((c=getc(input)) != EOF) {
if(!isalpha(c))
continue;
c=toupper(c);
//alphaKey[c-65]++;
putc(c,output);
//printf("count(%c) = %d\n",c,alphaKey[c-65]);
}
return 0;
}
